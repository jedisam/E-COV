{
  "name": "e-cov",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icon/corona.ico --prune=true --out=release-builds",
    "package-win": "electron-packager . --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icon/corona.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"E-COV\"",
    "package-linux" : "electron-packager . --overwrite --platform=linux --arch=x64 --icon=assets/icon/corona.ico --prune=true --out=release-builds"
  },
  "keywords": [],
  "author": "Yididya Samuel",
  "license": "ISC",
  "dependencies": {
    "electron": "^11.1.0"
  },
  "devDependencies": {
    "electron-packager": "^15.2.0"
  }
}
